<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="474.0" prefWidth="298.0" spacing="20.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.course_project.Controller">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Button fx:id="usersBtn" mnemonicParsing="false" onAction="#openUsers" prefHeight="41.0" prefWidth="221.0" text="Пользователь" />
      <Button fx:id="tablesBtnGuest" layoutX="48.0" layoutY="204.0" mnemonicParsing="false" onAction="#openTablesGuest" prefHeight="41.0" prefWidth="221.0" text="Столики" />
      <Button fx:id="dishesBtnGuest" layoutX="48.0" layoutY="212.0" mnemonicParsing="false" onAction="#openDishesGuest" prefHeight="41.0" prefWidth="221.0" text="Блюда" />
      <Button fx:id="exitBtn" mnemonicParsing="false" onAction="#exit" text="Выход" />
   </children>
</VBox>
