<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="320.0" prefWidth="400.0" spacing="20.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.course_project.Controller">
    <padding>
        <Insets bottom="20.0" left="7.0" right="20.0" top="7.0" />
    </padding>
   <AnchorPane prefHeight="298.0" prefWidth="374.0">
      <children>
         <Label layoutX="29.0" layoutY="22.0" prefHeight="18.0" prefWidth="248.0" text="Логин" />
          <TextField fx:id="userLogin" layoutX="29.0" layoutY="41.0" prefHeight="26.0" prefWidth="273.0" />
         <Label layoutX="31.0" layoutY="76.0" prefHeight="18.0" prefWidth="220.0" text="Пароль" />
         <PasswordField fx:id="userPassword" layoutX="29.0" layoutY="95.0" prefHeight="26.0" prefWidth="274.0" />
          <Button layoutX="130.0" layoutY="127.0" onAction="#login" text="Войти" />
         <Button fx:id="registerBtn" layoutX="89.0" layoutY="161.0" mnemonicParsing="false" onAction="#registrationBegin" prefHeight="26.0" prefWidth="130.0" text="Регистрация" />
         <Label fx:id="rightEnter" layoutX="56.0" layoutY="201.0" prefHeight="18.0" prefWidth="220.0" />
      </children>
   </AnchorPane>
</VBox>
